@{ ViewData["Title"] = "Show Employee"; }
@model practica.Models.CargarEmpleado
<table class="table">
    <thead>
        <tr>
            <th scope="col">@Model.Nombre</th>
            <th scope="col">@Model.Apellido</th>
            <th scope="col">@Model.AnioIngreso.ToString("dd/MM/yyyy")</th>
            <th scope="col">@Model.Nacimiento.ToString("dd/MM/yyyy")</th>
            <th scope="col">@Model.Sueldo</th>
        </tr>
    </thead>
    <tbody>
    </tbody>
</table>


